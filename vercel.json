{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/(assets/.*|canvaskit/.*|flutter.*js|main.dart.js|manifest.json|favicon.png|version.json)",
            "destination": "/build/web/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/build/web/index.html"
        }
    ]
}